{
  "name": "nina.fm-faceb",
  "version": "0.0.0",
  "description": "The official Nina.fm admin",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:api": "cd ../nina.fm-api && pnpm run start:dev",
    "dev:stack": "concurrently -k -n API,Front -c bgWhite,bgBlack \"pnpm run dev:api\" \"pnpm run dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "version:check": "changeset status",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "types:generate": "openapi-typescript http://localhost:4000/docs-json -o types/api-generated.d.ts --make-paths-enum",
    "types:generate-safe": "openapi-typescript http://localhost:4000/docs-json -o types/api-generated.d.ts --make-paths-enum --additional-properties",
    "types:extract": "NODE_NO_WARNINGS=1 node --loader ts-node/esm scripts/extract-globals.ts",
    "types:sync": "./scripts/generate-types.sh"
  },
  "dependencies": {
    "@date-io/luxon": "^3.2.0",
    "@nuxt/eslint": "1.2.0",
    "@pinia/nuxt": "^0.11.3",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/vue-query": "^5.83.0",
    "@tanstack/vue-table": "^8.21.2",
    "@vee-validate/nuxt": "^4.15.0",
    "@vee-validate/zod": "^4.15.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue-email/components": "^0.0.21",
    "@vue-email/render": "^0.0.9",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.5.0",
    "@vueuse/nuxt": "13.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.0.0",
    "h3": "^1.15.1",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.487.0",
    "luxon": "^3.5.0",
    "nuxt": "^4.2.1",
    "nuxt-color-picker": "^1.2.8",
    "nuxt-seo-utils": "7.0.7",
    "pinia": "^3.0.4",
    "reka-ui": "^2.2.0",
    "shadcn-nuxt": "2.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tw-animate-css": "^1.2.5",
    "uncrypto": "^0.1.3",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-easy-lightbox": "^1.19.0",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/lodash-es": "^4",
    "@types/luxon": "^3",
    "@types/node": "^22.13.13",
    "concurrently": "^9.2.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-vue": "^10.0.0",
    "nuxt-resend": "^0.0.2",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "query-string": "^9.1.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-deno-plugin": "^1.31.0",
    "vue-tsc": "^3.0.3"
  },
  "packageManager": "pnpm@10.13.1"
}
